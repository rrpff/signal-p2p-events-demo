{
  "scripts": {
    "start": "http-server public",
    "test": "jest",
    "build-protos": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. ./protos/*.proto",
    "build-assets": "browserify src/index.ts -p [ tsify ] -o public/assets/dist/bundle.js",
    "build": "sh ./scripts/build.sh",
    "watch": "chokidar \"src/**/*.{ts,proto}\" \"package.json\" -i \"src/protos/*.ts\" -c \"npm run build\"",
    "dev": "npm run build && concurrently \"npm run watch\" \"npm start\""
  },
  "devDependencies": {
    "@trust/webcrypto": "^0.9.2",
    "@types/bytebuffer": "^5.0.42",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "browserify": "^17.0.0",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.20.0",
    "http-server": "^0.12.3",
    "jest": "^26.6.3",
    "jest-environment-jsdom": "^26.6.2",
    "protoc-tools": "^3.11.3",
    "ts-jest": "^26.5.1",
    "ts-proto": "^1.65.0",
    "tsify": "^5.0.2",
    "typescript": "^4.1.5",
    "watchify": "^4.0.0"
  },
  "dependencies": {
    "bytebuffer": "^5.0.1",
    "choo": "^7.1.0",
    "ulid": "^2.3.0"
  }
}
